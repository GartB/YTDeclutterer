{
  "manifest_version": 2,
  "name": "Youtube DeClutterer",
  "version": "1.6",
  "description": "Hides branding and end-of-video pop-ups on YouTube",
  "author": "Garrett",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "settings-icon.png"
  ]
}
